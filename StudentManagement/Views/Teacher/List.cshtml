@model IEnumerable<StudentManagement.Models.Teacher>

@{
    ViewData["Title"] = "Teachers List";
}

<h2>Teachers List</h2>

<table class="table table-bordered mt-3">
    <thead>
        <tr>
            <th>Name</th>
            <th>Age</th>
            <th>Sex</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var teacher in Model)
        {
            <tr>
                <td>@teacher.Name</td>
                <td>@teacher.Age</td>
                <td>@teacher.Sex</td>
            </tr>
        }
    </tbody>
</table>
